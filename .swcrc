{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "jsx": false,
      "dynamicImport": false,
      "privateMethod": false,
      "functionBind": false,
      "exportDefaultFrom": false,
      "exportNamespaceFrom": false,
      "decorators": true,
      "decoratorsBeforeExport": false,
      "topLevelAwait": false,
      "importMeta": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "target": "es5",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false,
    "paths": {
      "@application/*": ["./application/*"],
      "@domain/*": ["./domain/*"],
      "@infra/*": ["./infra/*"],
      "@presentation/*": ["./presentation/*"],
      "@shared/*": ["./shared/*"],
      "@env": ["./env.ts"]
    },
    "baseUrl": "./src"
  },
  "module": {
    "type": "commonjs"
  },
  "minify": false
}
